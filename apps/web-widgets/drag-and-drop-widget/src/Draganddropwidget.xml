<?xml version="1.0" encoding="utf-8"?>
  <widget id="mendix.draganddropwidget.Draganddropwidget" pluginWidget="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../../node_modules/mendix/custom_widget.xsd">
    <name>Draganddropwidget</name>
    <description>DND Widget</description>
    <icon>
</icon>
    <properties>
        <propertyGroup caption="JSON">

            <property key="widgetJsonState" type="attribute">
                <caption>Widget Json State</caption>
                <description></description>
                <attributeTypes>
                    <attributeType name="String"/>
                </attributeTypes>
            </property>

             <property key="hasDataContent" type="widgets" required="true" dataSource="incomingData">
                <caption>Displayed When There is Data</caption>
                <description>Widgets using data source</description>
            </property>

             <property key="hasNoDataContent" type="widgets" >
                <caption>Displayed When There is No Data</caption>
                <description>Widgets using data source</description>
            </property>

        </propertyGroup>

        <propertyGroup caption="Actions">
            
            <property key="newParentAction" type="action">
                <caption>Dragged Into New Parent</caption>
                <description>Action called when dragged into from different Parent</description>
            </property>
            
            <property key="sameParentAction" type="action">
                <caption>Dragged in Same Parent</caption>
                <description>Action called when dragged to same Parent (Re-Order)</description>
            </property>

        </propertyGroup>

        <propertyGroup caption="Data and UUIDs">

            <property key="uuidStringContainer" type="string" required="true">
                <caption>Unique Container Name</caption>
                <description></description>
            </property>
            
            <property key="isParent" type="boolean" defaultValue="true">
                <caption>Is Parent Container</caption>
                <description>Is Parent Container or Nested in another DND widget</description>
            </property>

            <property key="incomingData" type="datasource" isList="true">
                <caption>Data</caption>
                <description></description>
            </property>

            <property key="sortOn" type="attribute" dataSource="incomingData">
                <caption>Sort On</caption>
                    <description>Integer To Sort On</description>
                <attributeTypes>
                    <attributeType name="Integer"/>
                </attributeTypes>
            </property>

            <property key="sort" type="enumeration" defaultValue="asc">
                <caption>Sort Asc or Desc</caption>
                <description />
                <enumerationValues>
                    <enumerationValue key="asc">Ascending</enumerationValue>
                    <enumerationValue key="desc">Descending</enumerationValue>
                </enumerationValues>
            </property>

            <property key="isColumn" type="boolean" defaultValue="true">
                <caption>Display as Column</caption>
                <description>True - Flex Column || False - Flex Row</description>
            </property>

            <property key="uuidStringParent" type="attribute" required="false">
                <caption>Unique Parent</caption>
                <description>Unique UUID of Parent</description>
                <attributeTypes>
                    <attributeType name="String"/>
                </attributeTypes>
            </property>

            <property key="disableDrag" type="expression" defaultValue="false">
                <caption>Disable Drag</caption>
                <description></description>
                <returnType type="Boolean" />
            </property>

            <property key="acceptedUuid" type="object" isList="true" required="false">
                    <caption>Unique Container Name Accepts</caption>
                    <description>Unique Container Name Accepts Drags/Drops From</description>
                    <properties>
                        <propertyGroup caption="Object list group">
                            <property key="uuids" type="string" defaultValue=" ">
                                <caption>Unique Container Name</caption>
                                <description>Unique Container Name Accepts Drags/Drops From</description>
                            </property>
                        </propertyGroup>
                    </properties>
            </property>

            <property key="uuidStringParentExpression" type="expression" defaultValue="" required="false">
                <caption>Unique Parent Expression</caption>
                <description></description>
                <returnType type="String" />
            </property>

            <property key="uuidStringCurrent" type="attribute" dataSource="incomingData">
                <caption>Unique Iteration</caption>
                 <description>Unique UUID of Self</description>
                <attributeTypes>
                    <attributeType name="String"/>
            </attributeTypes>
            </property>
        </propertyGroup>
            <propertyGroup caption="Accessiblity">
                <property key="readParentTitle" type="attribute" required="false">
                    <caption>Parent Title</caption>
                    <description>Human readabile Text of Parent</description>
                    <attributeTypes>
                        <attributeType name="String"/>
                    </attributeTypes>
                </property>
                <property key="readTitle" type="attribute" dataSource="incomingData" required="false">
                    <caption>Self Title</caption>
                    <description>Human readabile Text of Self</description>
                    <attributeTypes>
                        <attributeType name="String"/>
                </attributeTypes>
                </property>
         </propertyGroup>
    </properties>
</widget>