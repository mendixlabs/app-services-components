    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^24.0.23",
    "@types/promise-queue": "^2.2.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webdriverio": "^4.13.3",
    "core-js": "^2.6.11",
    "coveralls": "^3.0.11",
    "identity-obj-proxy": "^3.0.0",
    "mendix-client": "^7.15.8",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "dependencies": {
    "@jeltemx/mendix-react-widget-utils": "^0.7.0",
    "antd": "^4.8.5",
    "array-to-tree": "^3.3.2",
    "big.js": "^5.2.2",
    "classnames": "^2.2.6",
    "debounce": "^1.2.0",
    "mobx": "^4.15.7",
    "mobx-react": "^6.3.1",
    "promise-queue": "^2.2.5",
    "store2": "^2.12.0"
  }
