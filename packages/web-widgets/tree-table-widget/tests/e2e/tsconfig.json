{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "baseUrl": ".",
      "rootDir": ".",
      "types": ["node", "@wdio/sync", "jest"],
      "emitDecoratorMetadata": true,
